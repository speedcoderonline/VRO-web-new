[data-page='contact']
	.main-content
		.contact-container
			padding 80px 0
			h3, p
				color black
			.contact-wrapper
				display flex
				width 100%
				max-width 1100px
				padding 40px 0
				margin auto
				flex-wrap wrap
				justify-content center
				border-bottom 5px solid rgba(black, .05)			
				.contact-cell
					padding 20px 40px
					h3
						font-size 200%
					p
						margin-top 10px
				.line-break
					flex-basis 100%
					height 20px
			.president-first-lady
				justify-content center